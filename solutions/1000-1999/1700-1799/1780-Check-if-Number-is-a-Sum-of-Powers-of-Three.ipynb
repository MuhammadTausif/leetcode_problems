   "source": [
    "class Solution:\n",
    "  def checkPowersOfThree(self, n: int) -> bool:\n",
    "    while n > 1:\n",
    "      n, r = divmod(n, 3)\n",
    "      print(f'r: {r}, n: {n}')\n",
    "      if r == 2:\n",
    "        return False\n",
    "    return True"
   ]
  },
